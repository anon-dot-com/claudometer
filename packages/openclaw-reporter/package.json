{
  "name": "@claudometer/openclaw-reporter",
  "version": "1.0.0",
  "description": "OpenClaw plugin to report usage metrics to Claudometer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "openclaw": {
    "extensions": ["dist/index.js"]
  },
  "scripts": {
    "build": "tsc",
