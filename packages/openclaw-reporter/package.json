    "claudometer",
    "metrics",
    "usage-tracking"
  ],
  "author": "Claudometer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/claudometer/openclaw-reporter"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "openclaw": ">=1.0.0"
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "skills"
